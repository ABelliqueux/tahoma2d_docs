#: ../../source/installing_opentoonz.rst:149
msgid "Debian GNU/Linux"
msgstr ""

#: ../../source/installing_opentoonz.rst:151
msgid "Download PGP signing key package."
msgstr ""

#: ../../source/installing_opentoonz.rst:153
msgid ""
"``wget http://www.deb-multimedia.org/pool/main/d/deb-multimedia-keyring"
"/deb-multimedia-keyring_2016.8.1_all.deb``"
msgstr ""

#: ../../source/installing_opentoonz.rst:155
msgid "Install PGP package."
msgstr ""

#: ../../source/installing_opentoonz.rst:157
msgid "``sudo dpkg -i deb-multimedia-keyring_2016.8.1_all.deb``"
msgstr ""

#: ../../source/installing_opentoonz.rst:159
msgid "Add repository address to your sources list."
msgstr ""

#: ../../source/installing_opentoonz.rst:161
msgid ""
"``sudo echo \"deb http://www.deb-multimedia.org stable main\" | tee -a "
"/etc/apt/sources.list`` Builds for testing and sid also avalaible."
msgstr ""

#: ../../source/installing_opentoonz.rst:163
msgid "Debian packages avalaible only for amd64, arm64 and i386 architectures."
msgstr ""

#: ../../source/installing_opentoonz.rst:165
msgid "Install OpenToonz."
msgstr ""

#: ../../source/installing_opentoonz.rst:167
msgid "``sudo apt update && apt install opentoonz opentoonz-data``"
msgstr ""

#: ../../source/installing_opentoonz.rst:169
msgid "For updating enter ``sudo apt update && apt upgrade``."
msgstr ""

