msgid "//Save the result prompting a status message"
msgstr ""

#: ../../source/toonzscript.rst:737
msgid "``b.save(dir2+\"rottoad3..tif\")``"
msgstr ""

#: ../../source/toonzscript.rst:739
msgid "``print(\"saved\\n\",b);``"
msgstr ""

